{
	"schema_version": 1,
	"quilt_loader": {
		"group": "net.paperfish",
		"id": "moonbits",
		"version": "${version}",
		"metadata": {
			"name": "Moonbits",
			"description": "A minecraft mod designed as a fanmade branch of updates and content :D",
			"contributors": {
				"paperfish": "owner",
				"SameButDifferent": "programmer"
			},
			"contact": {
				"sources": "https://github.com/PaperfishStudios/Moonbits"
			},
			"icon": "assets/moonbits/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "net.paperfish.moonbits.Moonbits",
			"client_init": "net.paperfish.moonbits.client.MoonbitsClient",
			"fabric-datagen": "net.paperfish.moonbits.data.MBDataGenerator",
			"booking_it:recipe_book": "net.paperfish.moonbits.recipe.MBRecipeBook",
