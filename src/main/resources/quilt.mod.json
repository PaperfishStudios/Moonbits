				"id": "quilted_fabric_api",
				"versions": ">=1.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19"
			},
			{
				"id": "booking_it",
				"versions": "*"
			},
			{
				"id": "terrablender",
				"versions": ">=1.1.0.96"
			}
		]
	},
	"mixin": "moonbits.mixins.json",
	"accessWidener" : "moonbits.accesswidener"
}
