            "apply": { "model": "moonbits:block/warped_planter_box_side_b", "y": 270, "uvlock": true }
        },
        {   "when": { "west": "false", "south": "false", "sw": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_inner", "y": 270, "uvlock": true }
        },
        {   "when": { "west": "false", "south": "false", "sw": "false", "raised": "false" },
