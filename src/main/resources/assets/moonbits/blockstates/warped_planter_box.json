        {   "when": { "south": "false", "east": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_side_b", "y": 180, "uvlock": true }
        },
        {   "when": { "south": "false", "east": "false", "se": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_inner", "y": 180, "uvlock": true }
        },
        {   "when": { "south": "false", "east": "false", "se": "false", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box", "y": 180, "uvlock": true }
