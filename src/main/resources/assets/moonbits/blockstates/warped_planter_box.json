        {   "when": { "raised": "true" },
            "apply": { "model": "moonbits:block/warped_planter_box_raised", "uvlock": true }
        },
		
        {   "when": { "north": "true", "west": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_outer", "uvlock": true }
        },
        {   "when": { "north": "true", "west": "false", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_side", "uvlock": true }
        },
        {   "when": { "north": "false", "west": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_side_b", "uvlock": true }
        },
        {   "when": { "north": "false", "west": "false", "nw": "true", "raised": "false" },
            "apply": { "model": "moonbits:block/warped_planter_box_inner", "uvlock": true }
